# Univariate Analysis of Original Variables: Gender

# Frequency table
library(ggplot2)

# Frequency table
# Univariate Analysis of Original Variables: Gender

# Frequency table
gender_table <- table(data$gender)
print(gender_table)

# Bar plot
ggplot(data, aes(x = gender)) +
  geom_bar(fill = "#ecf2f9") +
  labs(title = "Distribution of Gender", x = "Gender", y = "Count") +
  theme_minimal()


